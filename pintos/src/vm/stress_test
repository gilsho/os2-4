echo "Executing $1 iterations of 'make check' with jitter..."
rm -rf ./stress_results/
mkdir ./stress_results/
for i in `seq 1 $1`
do
  echo "iteration: $i"
  make clean
  make
  cd ./build/
  make check PINTOSOPS='-j %i' > ../stress_results/test_$i.out
  cd ..
done
grep -r "FAIL" ./stress_results/. > failures.tmp
sort -u failures.tmp > ./stress_results/failures.out
rm -f failures.tmp
echo "Stress testing complete."
